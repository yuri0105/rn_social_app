{
  "expo": {
    "name": "Expo example for react-native-gl-image-filters",
    "slug": "example-expo",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.3.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
